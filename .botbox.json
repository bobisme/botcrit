{
  "version": "1.0.11",
  "project": {
    "name": "botcrit",
    "type": [
      "cli"
    ],
    "channel": "botcrit",
    "defaultAgent": "botcrit-dev"
  },
  "tools": {
    "beads": true,
    "maw": true,
    "crit": true,
    "botbus": true,
    "botty": true
  },
  "review": {
    "enabled": true,
    "reviewers": [
      "security"
    ]
  },
  "pushMain": false,
  "agents": {
    "dev": {
      "model": "opus",
      "pause": 2,
      "timeout": 900,
      "maxLoops": 20
    },
    "worker": {
      "model": "haiku",
      "timeout": 600
    },
    "reviewer": {
      "model": "opus",
      "pause": 2,
      "timeout": 600,
      "maxLoops": 20
    }
  }
}
